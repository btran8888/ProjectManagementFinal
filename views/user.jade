extends layout

block content
  div.container
    div.row
      div.col-sm-6.col-md-4.col-md-offset-4
        #user
          h1.text-left.login-title Projects assigned by you to user #{assignedToUser}:
            div.projects
              ul.project-details
                each project in projects
                   li 
                     a(href='/project/#{project.name}') #{project.name}
