
var mongoose = require('mongoose');

module.exports = mongoose.model('User',{
	id: String,
	username: String,
	password: String,
	email: String,
	firstName: String,
	lastName: String
});

module.exports = mongoose.model( 'Project', {
	name: String,
	ownerEmail: String,
	assignedToEmail: String,
	needhours: Number,
        workedhours: Number
